{
  "name": "@antribute/example-adk-todo-list",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun run generate:antribute && bun run --bun next build",
    "clean": "rimraf .next && rimraf .turbo &&  rimraf coverage && rimraf node_modules && rimraf test-results && rimraf .eslintcache",
    "dev": "next dev",
    "generate:antribute": "bun run --bun ../../node_modules/.bin/antribute-backend generate",
    "lint": "bun run generate:antribute && bun run --bun eslint --cache ./src",
    "start": "next start",
    "test": "bun run generate:antribute && vitest"
  },
  "dependencies": {
    "@antribute/backend-auth-nextauth": "workspace:*",
    "@antribute/backend-core": "workspace:*",
    "@antribute/backend-graphql-pothos": "workspace:^",
    "@antribute/backend-orm-prisma": "workspace:^",
    "@antribute/backend-perms-auth0-fga": "workspace:^",
    "@antribute/backend-server-nextjs": "workspace:^",
    "@auth0/fga": "^0.9.1",
    "@graphql-yoga/plugin-disable-introspection": "^0.8.1",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-prisma": "3.51.1",
    "@pothos/plugin-prisma-utils": "^0.8.1",
    "@pothos/plugin-scope-auth": "^3.19.1",
    "@prisma/client": "^4.8.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "graphql-yoga": "^3.1.1",
    "next": "^13.5.4",
    "next-auth": "^4.22.1",
    "prisma": "^4.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@antribute/config": "workspace:*",
    "@types/koa": "^2.13.9",
    "@types/node": "20.1.7",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.40.0",
    "happy-dom": "^7.7.0",
    "prettier": "2.8.7",
    "typescript": "^5.0.4",
    "vitest": "^0.25.3"
  }
}
