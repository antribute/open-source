{
  "name": "@antribute/zephyr-core",
  "description": "Core TailwindCSS Configurations and ClassNames for Zephyr Components",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./tailwindConfig": "./tailwind.config.cjs",
    "./zephyr-core.css": "./zephyr-core.css"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "./tailwind.config.cjs",
    "./zephyr-core.css"
  ],
  "scripts": {
    "build": "pnpm run build:classnames && pnpm run build:config",
    "build:config": "tsup src/tailwindConfig.ts --format cjs --dts --minify --no-config --sourcemap",
    "build:classnames": "tsup",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf text-results && rimraf .eslintcache",
    "dev": "concurrently \"pnpm run build:classnames --watch\" \"pnpm run build:config --watch\" --raw",
    "lint": "eslint --cache src/**",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "design system",
    "tailwindcss",
    "typescript"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "devDependencies": {
    "@antribute/config": "workspace:*",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@vitest/coverage-c8": "^0.25.3",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "happy-dom": "^7.7.0",
    "postcss": "^8.4.19",
    "prettier": "2.8.2",
    "tailwind-group-variant": "^1.0.0",
    "tailwindcss": "^3.3.1",
    "ts-toolbelt": "^9.6.0",
    "tsup": "^6.5.0",
    "type-fest": "^3.5.1",
    "typescript": "^5.0.0-beta",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "autoprefixer": ">= 10.0.0 < 11",
    "postcss": ">= 8.0.0 < 9",
    "tailwindcss": ">= 3.0.0 < 4"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.0.7",
    "@iconify-json/fa6-brands": "^1.1.10",
    "@iconify-json/heroicons": "^1.1.10",
    "@radix-ui/react-toast": "^1.1.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "cal-sans": "^1.0.1",
    "clsx": "^1.2.1",
    "dot-path-value": "^0.0.6",
    "hex-rgb": "^5.0.0",
    "lodash-es": "^4.17.21",
    "tailwindcss-radix": "^2.6.2",
    "twin.macro": "^3.1.0"
  }
}
