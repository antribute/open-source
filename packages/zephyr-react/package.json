{
  "name": "@antribute/zephyr-react",
  "description": "React Component Library for the Zephyr Design System",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "pnpm run build:components && pnpm run build:helpers && pnpm run build:index",
    "build:index": "tsup --config ./tsup.config.js",
    "build:components": "tsup --config ./tsup.config.components.js",
    "build:helpers": "tsup --config ./tsup.config.helpers.js",
    "build-storybook": "storybook build",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf storybook-static && rimraf text-results && rimraf .eslintcache",
    "dev": "concurrently \"pnpm run build:components --watch\" \"pnpm run build:helpers --watch\" \"pnpm run build:index --watch --ignore-watch='src/components'\" --raw",
    "lint": "eslint  ./src",
    "storybook": "storybook dev -p 6006 --no-open",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "design system",
    "tailwindcss",
    "typescript"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
