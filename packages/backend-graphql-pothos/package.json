{
  "name": "@antribute/backend-graphql-pothos",
  "description": "Adds Pothos GraphQL Support to the Antribute Backend",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist/**"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf text-results && rimraf .eslintcache",
    "dev": "pnpm run build --watch",
    "lint": "eslint --cache ./src",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "antribute-backend",
    "antribute-backend-graphql",
    "pothos"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "dependencies": {
    "execa": "^7.1.1",
    "fast-glob": "^3.2.12",
    "tsx": "^3.12.7"
  },
  "devDependencies": {
    "@antribute/backend-core": "workspace:0.1.0",
    "@antribute/config": "workspace:0.1.0",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-prisma": "3.51.1",
    "@pothos/plugin-prisma-utils": "^0.8.1",
    "@pothos/plugin-scope-auth": "^3.19.1",
    "@prisma/client": "^4.8.1",
    "@types/node": "20.1.7",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.40.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "prettier": "2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "@antribute/backend-core": "*",
    "@pothos/core": ">= 3.0.0 < 4",
    "@pothos/plugin-prisma": ">= 3.0.0 < 4",
    "@pothos/plugin-prisma-utils": "^0.8.0",
    "@pothos/plugin-scope-auth": ">= 3.0.0 < 4",
    "@prisma/client": ">= 4.0.0 < 5",
    "graphql": ">= 16.0.0 < 17",
    "graphql-scalars": ">= 1.0.0 < 2"
  },
  "peerDependenciesMeta": {
    "@pothos/plugin-prisma": {
      "optional": true
    },
    "@pothos/plugin-prisma-utils": {
      "optional": true
    },
    "@pothos/plugin-scope-auth": {
      "optional": true
    },
    "@prisma/client": {
      "optional": true
    }
  }
}
