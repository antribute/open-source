{
  "name": "@antribute/backend-cli",
  "description": "A CLI that does a lot of the Antribute Backend's heavy lifting and code generation",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "antribute-backend": "./dist/index.js"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo && rimraf .next && rimraf node_modules && rimraf build",
    "cli": "node ./dist/index.js",
    "dev": "pnpm run build --watch",
    "lint": "eslint --cache src/**",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "graphql",
    "nextjs"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "devDependencies": {
    "@antribute/config": "workspace:*",
    "@types/node": "18.11.9",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.28.0",
    "graphql": "^16.6.0",
    "happy-dom": "^7.7.0",
    "prettier": "2.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "@clack/core": "^0.3.2",
    "@pothos/core": "^3.24.0",
    "@pothos/plugin-prisma": "0.0.0-preview-20230107003824",
    "@prisma/client": "^4.8.1",
    "cmd-ts": "^0.12.1",
    "execa": "^7.1.1",
    "fast-glob": "^3.2.12",
    "graphql-yoga": "^3.1.1",
    "picocolors": "^1.0.0",
    "prisma": "^4.11.0",
    "prisma-import": "^1.0.4",
    "rimraf": "^4.4.1"
  },
  "peerDependencies": {
    "graphql": ">= 16.0.0 < 17"
  }
}
