{
  "name": "@antribute/backend-orm-prisma",
  "description": "Antribute Backend ORM Adapter for Prisma",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf text-results && rimraf .eslintcache",
    "dev": "pnpm run build --watch",
    "lint": "eslint --cache src/**",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "antribute-backend",
    "prisma"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "dependencies": {
    "execa": "^7.1.1",
    "prisma-import": "^1.0.4",
    "rimraf": "^4.4.1"
  },
  "devDependencies": {
    "@antribute/backend-core": "workspace:*",
    "@antribute/config": "workspace:*",
    "@types/node": "18.11.9",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.28.0",
    "prettier": "2.8.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "@antribute/backend-core": "*",
    "@pothos/plugin-prisma": ">= 3.0.0 < 4",
    "@prisma/client": ">= 4.0.0 < 5",
    "prisma": ">= 4.0.0 < 5"
  },
  "peerDependenciesMeta": {
    "@pothos/plugin-prisma": {
      "optional": true
    }
  }
}
