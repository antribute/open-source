{
  "name": "@antribute/backend-orm-prisma",
  "description": "Adds Prisma Support to the Antribute Backend",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "bun": "./src/index.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "dist/**",
    "src/**"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bunx --bun unbuild",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf test-results && rimraf .eslintcache",
    "lint": "eslint --cache ./src",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "antribute-backend",
    "antribute-backend-orm",
    "prisma"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "dependencies": {
    "execa": "^7.1.1",
    "prisma-import": "^1.0.4",
    "rimraf": "^4.4.1"
  },
  "devDependencies": {
    "@antribute/backend-core": "workspace:*",
    "@antribute/config": "workspace:*",
    "@prisma/client": "^4.8.1",
    "@types/node": "20.1.7",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.40.0",
    "prettier": "2.8.7",
    "prisma": "^4.13.0",
    "typescript": "^5.0.4",
    "unbuild": "^2.0.0",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "@antribute/backend-core": "*",
    "@prisma/client": ">= 4.0.0 < 5",
    "prisma": ">= 4.0.0 < 5"
  }
}
