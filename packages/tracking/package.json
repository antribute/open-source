{
  "name": "@antribute/tracking",
  "description": "An Antribute abstraction over Mixpanel",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/client.cjs",
  "exports": {
    ".": {
      "require": "./dist/client.cjs",
      "import": "./dist/client.js"
    },
    "./server": {
      "require": "./dist/server.cjs",
      "import": "./dist/server.js"
    }
  },
  "types": "./dist/client.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf text-results && rimraf .eslintcache",
    "dev": "pnpm run build:watch",
    "lint": "eslint --cache src/**",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "mixpanel",
    "react",
    "typescript"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "devDependencies": {
    "@antribute/config": "workspace:*",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/react": "^13.4.0",
    "@types/cli-color": "^2.0.2",
    "@types/mixpanel-browser": "^2.38.0",
    "@types/react": "18.0.25",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.28.0",
    "happy-dom": "^7.7.0",
    "prettier": "2.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^6.5.0",
    "typescript": "^5.0.0-beta",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "@antribute/utils-react": "workspace:*",
    "@testing-library/user-event": "^14.4.3",
    "cli-color": "^2.0.3",
    "mixpanel": "^0.17.0",
    "mixpanel-browser": "^2.45.0"
  },
  "peerDependencies": {
    "react": ">= 18.0.0 < 19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
