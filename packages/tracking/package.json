{
  "name": "@antribute/tracking",
  "description": "An Antribute abstraction over Mixpanel",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "bun": "./src/client.ts",
      "require": "./dist/client.js",
      "import": "./dist/client.mjs",
      "types": "./dist/client.d.ts"
    },
    "./server": {
      "bun": "./src/server.ts",
      "require": "./dist/server.js",
      "import": "./dist/server.mjs",
      "types": "./dist/server.d.ts"
    }
  },
  "types": "./dist/client.d.ts",
  "files": [
    "dist/**",
    "src/**"
  ],
  "scripts": {
    "build": "bunx --bun unbuild",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf test-results",
    "lint": "eslint --cache ./src"
  },
  "keywords": [
    "antribute",
    "mixpanel",
    "react",
    "typescript"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "dependencies": {
    "@testing-library/user-event": "^14.4.3",
    "mixpanel": "^0.17.0",
    "mixpanel-browser": "^2.45.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@antribute/config": "workspace:*",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/react": "^13.4.0",
    "@types/cli-color": "^2.0.2",
    "@types/mixpanel-browser": "^2.38.0",
    "@types/react": "^18.2.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4",
    "unbuild": "^2.0.0",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^3.6.0"
  },
  "peerDependencies": {
    "react": ">= 18.0.0 < 19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
