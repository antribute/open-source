{
  "name": "@antribute/graphql-logger-axiom",
  "description": "A package that sends GraphQL logs from @antribute/graphql-nextjs to Axiom",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo && rimraf node_modules && rimraf build",
    "dev": "pnpm run build --watch",
    "lint": "eslint --cache src/**",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "axiom",
    "graphql"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "devDependencies": {
    "@antribute/config": "workspace:*",
    "@types/cli-color": "^2.0.2",
    "@types/node": "18.11.9",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.36.0",
    "happy-dom": "^7.7.0",
    "prettier": "2.8.7",
    "tsup": "^6.5.0",
    "typescript": "^5.0.0-beta",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "cli-color": "^2.0.3"
  }
}
