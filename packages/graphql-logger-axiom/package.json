  "devDependencies": {
    "@antribute/config": "workspace:*",
    "@types/node": "18.11.9",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.28.0",
    "happy-dom": "^7.7.0",
    "prettier": "2.8.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "@axiomhq/axiom-node": "^0.10.0"
  }
