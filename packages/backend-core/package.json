{
  "name": "@antribute/backend-core",
  "description": "The Antribute Backend Framework",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "antribute-backend": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist/**"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf text-results && rimraf .eslintcache",
    "dev": "pnpm run build --watch",
    "lint": "eslint --cache ./src",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "antribute-backend"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "dependencies": {
    "bundle-require": "^4.0.1",
    "cmd-ts": "^0.12.1",
    "esbuild": "^0.17.18",
    "handlebars": "^4.7.7",
    "joycon": "^3.1.1",
    "lodash-es": "^4.17.21",
    "picocolors": "^1.0.0",
    "rimraf": "^4.4.1"
  },
  "devDependencies": {
    "@antribute/config": "workspace:*",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "20.1.7",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.40.0",
    "prettier": "2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.25.3"
  }
}
