{
  "name": "@antribute/backend-perms-permify",
  "description": "Adds Permify Support to the Antribute Backend",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "bun": "./src/index.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "dist/**",
    "src/**"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bunx --bun unbuild",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf test-results && rimraf .eslintcache",
    "lint": "eslint --cache ./src",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "antribute-backend",
    "antribute-backend-perms",
    "permify"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "devDependencies": {
    "@antribute/backend-core": "workspace:*",
    "@antribute/config": "workspace:*",
    "@permify/permify-node": "^0.3.2",
    "@types/node": "20.1.7",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.40.0",
    "prettier": "2.8.7",
    "typescript": "^5.0.4",
    "unbuild": "^2.0.0",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "@antribute/backend-core": "*",
    "@permify/permify-node": ">= 0.3.2 < 1"
  }
}
