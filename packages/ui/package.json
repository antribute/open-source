{
  "name": "@antribute/ui",
  "description": "The Antribute Design Language and Component Library",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./tailwindConfig": {
      "bun": "./src/config/tailwindConfig.ts",
      "require": "./dist/config/tailwindConfig.js",
      "import": "./dist/config/tailwindConfig.mjs",
      "types": "./dist/config/tailwindConfig.d.ts"
    },
    "./antribute.css": "./antribute.css"
  },
  "files": [
    "dist/**",
    "src/**"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bunx --bun unbuild",
    "build-storybook": "bunx --bun storybook build",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf storybook-static && rimraf test-results && rimraf .eslintcache",
    "dev": "bunx --bun unbuild --watch",
    "lint": "bunx --bun eslint --cache ./src",
    "storybook": "bunx --bun storybook dev -p 6006 --no-open",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "design system",
    "react",
    "react-aria",
    "tailwindcss",
    "typescript"
