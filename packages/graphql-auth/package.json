{
  "name": "@antribute/graphql-auth",
  "description": "A graphql-yoga context creator for NextJS Auth and Scoped Permissions",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo && rimraf .next && rimraf node_modules && rimraf build",
    "dev": "pnpm run build --watch",
    "lint": "eslint --cache src/**",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "nextauth",
    "graphql"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "devDependencies": {
    "@antribute/config": "workspace:*",
    "@types/node": "18.11.9",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.36.0",
    "happy-dom": "^7.7.0",
    "next-auth": "^4.18.8",
    "prettier": "2.8.7",
    "tsup": "^6.5.0",
    "typescript": "^5.0.2",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "next-auth": ">= 4.0.0 < 5"
  }
}
