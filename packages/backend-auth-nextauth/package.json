{
  "name": "@antribute/backend-auth-nextauth",
  "description": "Adds NextAuth.js Support to the Antribute Backend",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "bun": "./src/index.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "dist/**",
    "src/**"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bunx --bun unbuild",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf test-results && rimraf .eslintcache",
    "lint": "eslint --cache ./src",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "antribute-backend",
    "antribute-backend-auth",
    "nextauth"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "devDependencies": {
    "@antribute/backend-core": "workspace:*",
    "@antribute/config": "workspace:*",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@types/node": "20.1.7",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.40.0",
    "next-auth": "^4.22.1",
    "prettier": "2.8.7",
    "typescript": "^5.0.4",
    "unbuild": "^2.0.0",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "@antribute/backend-core": "*",
    "@next-auth/prisma-adapter": ">= 1.0.0 < 2",
    "next-auth": ">= 4.0.0 < 5"
  },
  "peerDependenciesMeta": {
    "@next-auth/prisma-adapter": {
      "optional": true
    }
  }
}
