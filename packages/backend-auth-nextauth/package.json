{
  "name": "@antribute/backend-auth-nextauth",
  "description": "Antribute Backend Authentication Adapter for NextAuth.js",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
