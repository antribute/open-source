{
  "name": "@antribute/config",
  "description": "Common configuration files used between all Antribute projects",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "eslint/**",
    "nextjs/**",
    "postcss/**",
    "prettier/**",
    "tsconfig/**",
    "unbuild/**"
  ],
  "scripts": {
    "clean": "rimraf node_modules"
  },
  "keywords": [
    "antribute",
    "config",
    "eslint",
    "nextjs",
    "postcss",
    "prettier",
    "tsconfig",
    "typescript",
    "unbuild"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "dependencies": {
    "@vitejs/plugin-react": "^2.2.0",
    "bun-types": "latest",
    "nextjs-routes": "^1.0.4",
    "tsc-alias": "^1.8.8",
    "vite-tsconfig-paths": "^3.6.0"
  },
  "devDependencies": {
    "next": "^13.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4",
    "unbuild": "^2.0.0",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "eslint": ">= 8.51.0 < 9",
    "next": "^13.5.4",
    "typescript": ">= 5.0.4 < 6",
    "unbuild": ">= 2.0.0 < 3"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "tsc-alias": {
      "optional": true
    },
    "unbuild": {
      "optional": true
    }
  }
}
