{
  "name": "@antribute/backend",
  "description": "The Antribute Backend Framework, in convenient CLI form",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "antribute-backend": "./dist/index.js"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo &&  rimraf coverage && rimraf dist && rimraf node_modules && rimraf text-results && rimraf .eslintcache",
    "cli": "node ./dist/index.js",
    "dev": "pnpm run build --watch",
    "lint": "eslint --cache src/**",
    "test": "vitest"
  },
  "keywords": [
    "antribute",
    "graphql",
    "nextjs"
  ],
  "author": "Antribute, Inc. (https://www.antribute.com)",
  "license": "MIT",
  "devDependencies": {
    "@antribute/backend-core": "workspace:^",
    "@antribute/config": "workspace:*",
    "@types/node": "18.11.9",
    "@vitest/coverage-c8": "^0.25.3",
    "eslint": "^8.28.0",
    "graphql": "^16.6.0",
    "happy-dom": "^7.7.0",
    "prettier": "2.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "cmd-ts": "^0.12.1"
  },
  "peerDependencies": {
    "@antribute/backend-core": "*"
  }
}
